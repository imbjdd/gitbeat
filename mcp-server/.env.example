# GitBeat MCP Server Configuration

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# MCP Server Configuration
MCP_SERVER_NAME=gitbeat-music-generator
MCP_SERVER_VERSION=0.1.0

